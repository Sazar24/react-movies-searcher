{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "ordered-imports": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "no-unused-variable": false,
    "jsx-no-lambda": false,
    "curly": false,
    "member-access": [
      true,
      "no-public",
      "check-accessor"
    ]
  },
  "defaultSeverity": "warning"
}
